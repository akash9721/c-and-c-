#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,i;
    cout<<"enter any no:   ";
    cin>>n;
    int flag=0;
    for(int i=2;i<n;i++)
    {
        if(n%i==0)
        {
            flag=1;
            break;
        }
    }
    if(flag==0)
    {
        cout<<n<<" is a prime no";
    }
    else
    {
        cout<<n<<" is not prime no";
    }
    cout<<endl;
}
