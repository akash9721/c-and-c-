#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cout<<"enter no"<<endl;
    cin>>n;
    int a[n];
    cout<<"enter array element"<<endl;
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    cout<<"enter position from where you want rotate"<<endl;
    int d;
    cin>>d;
    for(int i=d;i<n;i++)
    {
        cout<<a[i]<<" ";
    }
    for(int i=0;i<d;i++)
    {
        cout<<a[i]<<" ";
    }


}
