#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int rem;
    int num=n;
    int res=0;
    while(n)
    {
       int rem=n%10;
        res=res+rem*rem*rem;
        n/=10;
    }
    if(num==res)
        cout<<"armstrog";
    else
    {
        cout<<"no";
    }

}
