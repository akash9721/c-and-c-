#include<bits/stdc++.h>
using namespace std;
int main()
{
	int n;
	cout<<"enter position"<<endl;
	cin>>n;
	int first=0,second=1;
	int third;
	if(n==1)
	{
		cout<<"0";
	}
	else if(n==2)
	{
		cout<<"1";
	}
	else
	{
	for(int i=3;i<=n;i++)
	{
		third=first+second;
	
		first=second;
		second=third;
		
	}
		cout<<third<<" ";
}
}
